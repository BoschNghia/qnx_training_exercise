@startuml

actor user as "User"

artifact hmi as "Input Device/Interface"

node OS {    
    agent driver as "Device Driver"
    agent shell as "Shell/Command Line Interface (CLI)"
    collections apps as "Executables"
}


user -right- hmi
hmi -right- driver
driver -[dashed]right-> shell
shell -[dashed]down-> apps: Invoke

@enduml
